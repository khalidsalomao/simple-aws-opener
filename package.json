{
  "name": "simple-aws-opener",
  "productName": "Simple Aws Opener",
  "version": "1.0.0",
  "main": "main/index.js",
  "license": "MIT",
  "scripts": {
    "start": "electron .",
    "build": "next build renderer && next export renderer",
    "build-electron": "./node_modules/.bin/electron ./node_modules/.bin/next build renderer && ./node_modules/.bin/electron ./node_modules/.bin/next export renderer",
    "dist": "yarn run build && build --dir",
    "lint": "yarn eslint renderer",
    "test": "yarn jest"
  },
  "build": {
    "files": [
      "**/*",
      "!renderer",
      "renderer/out"
    ]
  },
  "devDependencies": {
    "babel-eslint": "^8.0.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "electron": "^1.7.9",
    "electron-builder": "^19.47.1",
    "eslint": "^4.11.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-class-property": "^1.0.6",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "jest": "^21.2.1",
    "next": "latest",
    "react": "^16.1.1",
    "react-dom": "^16.1.1"
  },
  "dependencies": {
    "app-root-path": "^2.0.1",
    "axios": "^0.17.1",
    "dotenv": "^4.0.0",
    "electron-is-dev": "^0.3.0",
    "electron-json-storage": "^4.0.2",
    "electron-next": "^3.1.3",
    "mobx": "^3.3.1",
    "mobx-react": "^4.3.4",
    "react-md": "^1.2.8",
    "winston": "^2.4.0"
  },
  "jest": {
    "automock": false,
    "setupTestFrameworkScriptFile": "<rootDir>/jest-setup.js",
    "setupFiles": [
      "<rootDir>/shim.js"
    ]
  }
}
